{
  "compilerOptions": {
    "target": "es2016",
    "module": "CommonJS",
    "outDir": "./dist",
    "removeComments": true,
    "rootDir": "",
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strictPropertyInitialization": false,
    "forceConsistentCasingInFileNames": true
  },
  "exclude":[
    "./node_modules"
  ]
}